    "version": "0.2.0",
    "configurations": [
        {
            "name": "autonomy-production",
            "request": "launch",
            "type": "dart",
			"args": [
				"--flavor",
				"production"
			]
        },
        {
            "name": "autonomy-inhouse",
            "request": "launch",
            "type": "dart",
			"args": [
				"--flavor",
				"development"
			]
        },
        {
            "name": "autonomy-flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
				"--flavor",
				"prd"
			]
        },
        {
            "name": "autonomy-flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
				"--flavor",
				"prd"
			]
        }
    ]
}